{
	"scopeName": "tagged.templates.injection",
	"injectionSelector": "L:source.ts, L:source.tsx, L:source.js, L:source.jsx",
	"patterns": [
		{ "include": "#tag-json" },
		{ "include": "#tag-html" },
		{ "include": "#tag-css" },
		{ "include": "#tag-scss" },
		{ "include": "#tag-sql" },
		{ "include": "#tag-graphql" },
		{ "include": "#tag-yaml" },
		{ "include": "#tag-xml" },
		{ "include": "#tag-ts" },
		{ "include": "#tag-java" },
		{ "include": "#tag-ruby" },
		{ "include": "#tag-python" },
		{ "include": "#tag-php" },
		{ "include": "#tag-go" },
		{ "include": "#tag-csharp" },
		{ "include": "#tag-erb" },
		{ "include": "#tag-ejs" },
		{ "include": "#tag-handlebars" },
		{ "include": "#tag-mustache" },
		{ "include": "#tag-pug" },
		{ "include": "#tag-twig" },
		{ "include": "#tag-liquid" },
		{ "include": "#tag-slim" },
		{ "include": "#tag-haml" },
		{ "include": "#tag-jinja" },
		{ "include": "#tag-gemfile" },
		{ "include": "#tag-md" },
		{ "include": "#tag-gitignore" },
		{ "include": "#tag-env" },
		{ "include": "#tag-sh" }
	],
	"repository": {
		"interpolation": {
			"name": "meta.embedded.expression.ts",
			"begin": "\\$\\{",
			"beginCaptures": { "0": { "name": "punctuation.definition.template-expression.begin.ts" } },
			"end": "\\}",
			"endCaptures": { "0": { "name": "punctuation.definition.template-expression.end.ts" } },
			"patterns": [
				{ "include": "source.ts" },
				{ "include": "source.tsx" },
				{ "include": "source.js" },
				{ "include": "source.jsx" }
			]
		},

		"tag-json": {
			"name": "meta.embedded.block.json.tagged",
			"begin": "(?<=\\bjson)\\s*`",
			"end": "(?<!\\\\)`",
			"contentName": "source.json.embedded",
			"patterns": [{ "include": "#interpolation" }, { "include": "source.json" }]
		},

		"tag-html": {
			"name": "meta.embedded.block.html.tagged",
			"begin": "(?<=\\bhtml)\\s*`",
			"end": "(?<!\\\\)`",
			"contentName": "text.html.basic.embedded",
			"patterns": [{ "include": "#interpolation" }, { "include": "text.html.basic" }]
		},

		"tag-css": {
			"name": "meta.embedded.block.css.tagged",
